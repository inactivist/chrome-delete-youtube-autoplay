{
  "manifest_version": 2,

  "name": "Remove YouTube Autoplay",
  "description": "Isn't YouTube autoplay the worst? This removes it",
  "author": "krx",
  "version": "0.1",

  "permissions": ["http://www.youtube.com/"],

  "content_scripts": [{
    "matches": [
      "*://www.youtube.com/*"
    ],
    "js": [
      "thirdparty/jquery-3.1.1.slim.min.js",
      "disable_autoplay.js"
    ],
    "run_at": "document_end"
  }]
}
